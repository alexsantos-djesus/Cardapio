{
  "name": "cardapio",
  "version": "1.0.0",
  "scripts": {
    "dev": "tailwindcss -i ./styles/style.css -o ./styles/output.css --watch",
    "build": "tailwindcss -i ./styles/style.css -o ./styles/output.css --minify",
    "build:css": "node ./node_modules/tailwindcss/lib/cli.js -i ./styles/style.css -o ./styles/output.css --minify",
    "vercel-build": "npm run build:css"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.15"
  }
}
